    "start": "node scripts/customStart.js && react-scripts start",
    "build": "node scripts/customBuild.js && react-app-rewired build --basename kengreg && react-snap && node scripts/customCopy.js && node scripts/customUpdate.js && node scripts/customClean.js && node scripts/customRename.js && node scripts/customCss.js && node scripts/customCompile.js && npm run scss && npm run tw && node scripts/customMerge.js  && node scripts/rename.js",
    "buildprevious": "node scripts/customBuild.js && react-app-rewired build --basename kengreg && react-snap && node scripts/customCopy.js && node scripts/customUpdate.js && node scripts/customClean.js && node scripts/customRename.js && node scripts/customCss.js && node scripts/customCompile.js && npm run scss && npm run tw && node scripts/customMerge.js  && node scripts/rename.js && node scripts/concatChunks.js && node scripts/removeChunkReferences.js && node scripts/cleanupBuildJs.js && node scripts/removeImports.js && node scripts/customMoveImages.js && node scripts/purgeCSS.js",
