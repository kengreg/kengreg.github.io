    "eject": "react-scripts eject",
    "create": "node scripts/customCreate.js",
    "scss": "node-sass ./src/components/kengreg/index.scss -o build/static/css --output-style compressed",
    "tw": "tailwindcss build -o build/static/css/tailwind.css && postcss build/static/css/tailwind.css -o build/static/css/tailwind.min.css",
    "oldPurge": "purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
    "company": "live-server --no-browser --watch=./company/css --watch=./company/js",
    "company-scss": "node-sass ./company/src/scss/ -o ./company/css/",
    "watch-scss": "nodemon -e scss -x \"npm run company-scss\"",
    "watch-js": "nodemon -e js -x \"babel ./company/src/js -d ./company/js\"",
    "minifyhtml": "html-minifier --collapse-whitespace --remove-comments -o ./company/html/index.html ./company/index.html",
    "dev": "npx npm-run-all -p company-server watch-scss watch-js"
